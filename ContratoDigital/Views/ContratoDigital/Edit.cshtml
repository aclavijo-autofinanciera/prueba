@using Microsoft.AspNetCore.Identity
@using ContratoDigital.Areas.Identity.Data
@model ContratoDigital.Models.Contrato
@inject SignInManager<ContratoDigitalUser> SignInManager
@inject UserManager<ContratoDigitalUser> UserManager
@inject RoleManager<IdentityRole> RoleManager
@{
    //var userID = UserManager.GetUserId(User);
    var user = UserManager.Users.SingleOrDefault(x => x.Id == UserManager.GetUserId(User));
}
@{

    ViewData["Title"] = "Contrato Digital";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var tipoidentidad = (IQueryable<Estado>)ViewData["TipoIdentidad"];
    var sexo = (IQueryable<Estado>)ViewData["Sexo"];
    var estadoCivil = (IQueryable<Estado>)ViewData["EstadoCivil"];
    var departamento = (IQueryable<Estado>)ViewData["Departamento"];
}

<div class="container">
    <div class="row">
        <div class="col-12">
            <div style="background: url('../../../../images/background/anotherscooter.jpg') no-repeat center center fixed;-webkit-background-size: cover;-moz-background-size: cover;-o-background-size: cover;background-size: cover;min-height: 20rem;">
                <h1 class="text-white text-center text-shadow" data-aos="fade-in" data-aos-delay="500" style="max-width: 350px;position:absolute;left: 50%;top: 50%;transform: translate(-50%,-50%);">
                    Editar contrato
                </h1>
            </div>
        </div>
    </div>
    <div class="row">

        <div class="col-12">
            <form method="post" class="ContratoValidation bootstrapValidation" autocomplete="off">
                <input type="hidden" name="IdContrato" id="IdContrato" value="@Model.IdContrato" />
                <div class="form-row text-center">
                    <div class="col-12 col-sm-12 col-md-4 ">



                    </div>
                    <div class="col-12 col-sm-12 col-md-4 ">
                        <h1>Contrato Digital</h1>
                        <h5>Hoja de datos del contrato de adhesión</h5>
                    </div>
                    <div class="col-12 col-sm-12 col-md-4">
                        <label for="numero_de_contrato">Número de contrato</label>
                        <input type="text" id="numero_de_contrato" name="numero_de_contrato" readonly class="text-center form-control-plaintext form-control-lg azul font-weight-bold t-3" value="@Model.numero_de_contrato" required />
                    </div>
                </div>
                <!--#region suscriptor-->
                <section>
                    <div class="form-row">
                        <div class="col">
                            <div class="bg-azul p-3 mb-2 text-white text-center">
                                <h5 class="mb-0">Datos del suscriptor</h5>
                            </div>
                        </div>
                    </div>
                    <div class="form-row mt-4">
                        <div class="col-12 col-md-3">
                            <label for="primer_nombre">Primer nombre</label>
                            <input type="text" id="primer_nombre" name="primer_nombre" class="form-control form-control-lg text-uppercase" placeholder="Primer nombre" required value="@Model.primer_nombre" autocomplete="off">
                        </div>
                        <div class="col-12 col-md-3">
                            <label for="segundo_nombre">Segundo nombre</label>
                            <input type="text" id="segundo_nombre" name="segundo_nombre" class="form-control form-control-lg text-uppercase" placeholder="Segundo nombre" value="@Model.segundo_nombre" autocomplete="off">
                        </div>
                        <div class="col-12 col-md-3">
                            <label for="primer_apellido">Primer apellido</label>
                            <input type="text" id="primer_apellido" name="primer_apellido" class="form-control form-control-lg text-uppercase" placeholder="Primer apellido" required value="@Model.primer_apellido" autocomplete="off">
                        </div>
                        <div class="col-12 col-md-3">
                            <label for="segundo_apellido">Segundo apellido</label>
                            <input type="text" id="segundo_apellido" name="segundo_apellido" class="form-control form-control-lg text-uppercase" placeholder="Nombre (nombre completo o razón social)" value="@Model.segundo_apellido" autocomplete="off">
                        </div>

                    </div>
                    <div class="form-row mt-4">
                        <div class="col-12 col-md-4">
                            <label for="tipo_identificacion_suscriptor">Tipo de documento de identidad</label>
                            <select class="form-control form-control-lg" id="tipo_identificacion_suscriptor" name="tipo_identificacion_suscriptor" required autocomplete="off">
                                <option value="">Seleccione una opción</option>
                                @foreach (var item in tipoidentidad)
                                {
                                    @if (Model.tipo_documento_identidad_suscriptor == item.IdEstado.ToString())
                                    {
                                        <option selected="selected" value="@item.IdEstado">@item.Descripcion</option>
                                    }
                                    else
                                    {
                                        <option value="@item.IdEstado">@item.Descripcion</option>
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="documento_identidad_suscriptor">Número de documento de identidad</label>
                            <input type="text" class="form-control form-control-lg" placeholder="ie: 10000000" id="documento_identidad_suscriptor" name="documento_identidad_suscriptor" required value="@Model.documento_identidad_suscriptor" autocomplete="off" />
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="departamentoDocumentoSuscriptor">Departamento</label>
                            <select id="departamentoDocumentoSuscriptor" name="departamentoDocumentoSuscriptor" class="form-control form-control-lg">
                                <option value="">Seleccione un tipo</option>
                                @foreach (var item in departamento)
                                {
                                    if (item.IdEstado == int.Parse(Model.departamento_suscriptor))
                                    {
                                        <option value="@item.IdEstado" data-idsiicon="@item.IdSiicon" selected>@item.Descripcion</option>
                                    }
                                    else
                                    {
                                        <option value="@item.IdEstado" data-idsiicon="@item.IdSiicon">@item.Descripcion</option>
                                    }


                                }
                            </select>
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="procedencia_documento_identidad_suscriptor">De:</label>
                            <select class="form-control form-control-lg text-uppercase" id="procedencia_documento_identidad_suscriptor" name="procedencia_documento_identidad_suscriptor" required autocomplete="off">
                                <option value="">Seleccione una opción</option>
                                <option value="@Model.procedencia_documento_identidad_suscriptor" selected>@ViewData["ProcedenciaIdentidadSuscriptor"]</option>
                            </select>
                        </div>
                    </div>
                </section>
                <!--#endregion-->
                <!--#region representate legal suscriptor-->

                <div class="form-row mt-2 @(Model.documento_identidad_representante_legal > 0 ? Html.Raw("d-none") : Html.Raw("")) ">
                    <div class="col">
                        <a class="btn btn-azul text-white" id="isRepreentanteLegal"><i class="fas fa-plus"></i> Agregar representante legal</a>
                    </div>
                </div>

                <section class="@(Model.documento_identidad_representante_legal > 0 ? Html.Raw("") : Html.Raw("d-none") )" id="RepreentanteLegalSection" autocomplete="off">
                    <div class="card mt-4 border-azul ">
                        <div class="card-header text-white bg-azul">
                            Representante legal suscriptor
                        </div>
                        <div class="card-body">
                            <div class="form-row mt-1">
                                <div class="col-12">
                                    <label for="nombre_razon_social_representante_legal">Nombre representante legal (solo personas jurídicas)</label>
                                    <input type="text" class="form-control form-control-lg text-uppercase" id="nombre_razon_social_representante_legal" name="nombre_razon_social_representante_legal" placeholder="Nombre representante legal (solo personas jurídicas)" value="@Model.nombre_razon_social_representante_legal" autocomplete="off" />
                                </div>
                            </div>
                            <div class="form-row mt-4">
                                <div class="col-12 col-md-4">
                                    <label for="tipo_documento_representante_legal">Tipo de documento representante legal</label>
                                    <select class="form-control form-control-lg" id="tipo_documento_representante_legal" name="tipo_documento_representante_legal" autocomplete="off" autocomplete="off">
                                        <option value="">Seleccione una opción</option>
                                        @foreach (var item in tipoidentidad)
                                        {
                                            @if (Model.tipo_documento_representante_legal == item.IdEstado.ToString())
                                            {
                                                <option selected="selected" value="@item.IdEstado">@item.Descripcion</option>
                                            }
                                            else
                                            {
                                                <option value="@item.IdEstado">@item.Descripcion</option>
                                            }
                                        }
                                    </select>
                                </div>
                                <div class="col-12 col-md-4">
                                    <label for="documento_identidad_representante_legal">Número de documento representate legal</label>
                                    <input type="text" class="form-control form-control-lg" placeholder="ie: 10000000" id="documento_identidad_representante_legal" name="documento_identidad_representante_legal" value="@Model.documento_identidad_representante_legal" autocomplete="off" />
                                </div>
                                <div class="col-12 col-md-4">
                                    <label for="departamentoDocumentoRepresentanteLegal">Departamento</label>
                                    <select class="form-control form-control-lg" id="departamentoDocumentoRepresentanteLegal" name="departamentoDocumentoRepresentanteLegal" required autocomplete="off">
                                        <option value="">Seleccione un tipo</option>
                                        @foreach (var item in departamento)
                                        {
                                            <option value="@item.IdEstado" data-idsiicon="@item.IdSiicon">@item.Descripcion</option>
                                        }
                                    </select>
                                </div>
                                <div class="col-12 col-md-4">
                                    <label for="procedencia_documento_identidad_representante_legal">De:</label>
                                    <select class="form-control form-control-lg text-uppercase" id="procedencia_documento_identidad_representante_legal" name="procedencia_documento_identidad_representante_legal" value="@Model.procedencia_documento_identidad_representante_legal" autocomplete="off">
                                        <option vallue="">Seleccione un opción</option>
                                        <option value="@Model.procedencia_documento_identidad_representante_legal">@ViewData["ProcedenciaIdentidadRepresentanteLegal"]</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>

                <!--#endregion-->
                <!--#region datos del sucriptor-->

                <section>
                    <div class="form-row mt-4">
                        <div class="col-12 col-md-3">
                            <label for="fecha_nacimiento_suscriptor">Fecha de nacimiento</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="fecha_nacimiento_suscriptor_addon"><i class="far fa-calendar-alt"></i></span>
                                </div>
                                <input type="text" id="fecha_nacimiento_suscriptor" name="fecha_nacimiento_suscriptor" class="form-control form-control-lg" placeholder="ej: @DateTime.Today.ToString("yyyy-MM-dd")" aria-label="fecha_nacimiento_suscriptor" aria-describedby="fecha_nacimiento_suscriptor" required readonly value="@String.Format("{0:dd-MM-yyyy}", Model.fecha_nacimiento_suscriptor)" autocomplete="off" />
                            </div>

                        </div>
                        <div class="col-12 col-md-3">
                            <label for="lugar_nacimiento_suscriptor">Lugar de nacimiento</label>
                            <input type="text" id="lugar_nacimiento_suscriptor" name="lugar_nacimiento_suscriptor" class="form-control form-control-lg text-uppercase" required value="@Model.lugar_nacimiento_suscriptor" autocomplete="off" />
                        </div>
                        <div class="col-12 col-md-3">
                            <label for="sexo_suscriptor">Sexo</label>
                            <select class="form-control form-control-lg" id="sexo_suscriptor" name="sexo_suscriptor" required autocomplete="off">
                                <option value="">Seleccione una opción</option>
                                @foreach (var item in sexo)
                                {
                                    @if (Model.sexo_suscriptor == item.IdEstado.ToString())
                                    {
                                        <option selected="selected" value="@item.IdEstado">@item.Descripcion</option>
                                    }
                                    else
                                    {
                                        <option value="@item.IdEstado">@item.Descripcion</option>
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-12 col-md-3">
                            <label for="estado_civil_suscriptor">Estado civil</label>
                            <select class="form-control form-control-lg" id="estado_civil_suscriptor" name="estado_civil_suscriptor" required autocomplete="off">
                                <option value="">Seleccione una opción</option>
                                @foreach (var item in estadoCivil)
                                {
                                    @if (Model.estado_civil_suscriptor == item.IdEstado.ToString())
                                    {
                                        <option selected="selected" value="@item.IdEstado">@item.Descripcion</option>
                                    }
                                    else
                                    {
                                        <option value="@item.IdEstado">@item.Descripcion</option>
                                    }
                                }
                            </select>
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col">
                            <label for="direccion_domicilio_suscriptor">Dirección domicilio</label>
                            <input type="text" class="form-control form-control-lg text-uppercase" id="direccion_domicilio_suscriptor" name="direccion_domicilio_suscriptor" required value="@Model.direccion_domicilio_suscriptor" autocomplete="off" />
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col-12 col-md-4">
                            <label for="pais_suscriptor">País</label>
                            <input type="text" class="form-control-lg form-control-plaintext text-uppercase" readonly value="Colombia" id="pais_suscriptor" name="pais_suscriptor" autocomplete="off" />
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="departamento_suscriptor">Departamento</label>
                            <select class="form-control form-control-lg" id="departamento_suscriptor" name="departamento_suscriptor" required autocomplete="off">
                                <option value="">Seleccione una opción</option>
                                @foreach (var item in departamento)
                                {
                                    @if (Model.departamento_suscriptor == item.IdEstado.ToString())
                                    {
                                        <option selected="selected" value="@item.IdEstado" data-idsiicon="@item.IdSiicon">@item.Descripcion</option>
                                    }
                                    else
                                    {
                                        <option value="@item.IdEstado" data-idsiicon="@item.IdSiicon">@item.Descripcion</option>
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="ciudad_suscriptor">Ciudad</label>
                            <select type="text" class="form-control form-control-lg text-uppercase" id="ciudad_suscriptor" name="ciudad_suscriptor" required autocomplete="off" readonly>
                                <option value="">Seleccione una opción</option>
                                <option value="@Model.ciudad_suscriptor" selected>@ViewData["CiudadSuscriptor"]</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col-12 col-md-6">
                            <label for="telefono_suscriptor">Teléfono</label>
                            <input type="tel" class="form-control form-control-lg text-uppercase" id="telefono_suscriptor" name="telefono_suscriptor" required value="@Model.telefono_suscriptor" autocomplete="off" />
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="celular_suscriptor">Celular/Móvil</label>
                            <input type="tel" class="form-control form-control-lg text-uppercase" id="celular_suscriptor" name="celular_suscriptor" required value="@Model.celular_suscriptor" autocomplete="off" />
                        </div>
                    </div>

                    <div class="form-row mt-4 mb-2">
                        <div class="col">
                            <div class="bg-azul p-1 mb-2 text-white text-center">
                                <h5 class="mb-0 t-1">Datos laborales del suscriptor</h5>
                            </div>
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col-12 col-md-6">
                            <label for="empresa_empleadora_suscriptor">Empresa donde labora</label>
                            <input type="text" class="form-control form-control-lg text-uppercase" id="empresa_empleadora_suscriptor" name="empresa_empleadora_suscriptor" required value="@Model.empresa_empleadora_suscriptor" autocomplete="off" />
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="cargo_suscriptor">Cargo</label>
                            <input type="text" class="form-control form-control-lg text-uppercase" name="cargo_suscriptor" id="cargo_suscriptor" required value="@Model.cargo_suscriptor" autocomplete="off" />
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col-12 col-md-4">
                            <label for="ingresos_mensuales_suscriptor">Ingresos mensuales</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="ingresos_en_pesos_addon">$</span>
                                </div>
                                <input type="text" class="form-control form-control-lg" aria-label="ingresos_mensuales_suscriptor" aria-describedby="ingresos_en_pesos_addon" id="ingresos_mensuales_suscriptor" name="ingresos_mensuales_suscriptor" placeholder="0.00" required value="@String.Format("{0:0,0.00}", Model.ingresos_mensuales_suscriptor)" autocomplete="off" />
                            </div>
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="egresos_mensuales_suscriptor">Egresos mensuales</label>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="egresos_en_pesos_addon">$</span>
                                </div>
                                <input type="text" class="form-control form-control-lg" aria-label="egresos_mensuales_suscriptor" aria-describedby="egresos_en_pesos_addon" id="egresos_mensuales_suscriptor" name="egresos_mensuales_suscriptor" placeholder="0.00" required value="@String.Format("{0:0,0.00}", Model.egresos_mensuales_suscriptor)" autocomplete="off" />
                            </div>
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="otros_ingresos_suscriptor">Otros ingresos</label>
                            <div class="input-group">
                                <span class="input-group-prepend">
                                    <span class="input-group-text" id="otros_ingresos_en_pesos_addon">$</span>
                                </span>
                                <input type="text" class="form-control form-control-lg" aria-label="otros_ingresos_suscriptor" aria-describedby="otros_ingresos_en_pesos_addon" id="otros_ingresos_suscriptor" name="otros_ingresos_suscriptor" placeholder="0.00" required value="@String.Format("{0:0,0.00}", Model.otros_ingresos_suscriptor)" autocomplete="off" />
                            </div>
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col">
                            <label for="direccion_empleo_suscriptor">Dirección oficina (laboral)</label>
                            <input type="text" class="form-control form-control-lg text-uppercase" id="direccion_empleo_suscriptor" name="direccion_empleo_suscriptor" required value="@Model.direccion_empleo_suscriptor" autocomplete="off" />
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col-12 col-md-4">
                            <label for="pais_laboral_suscriptor">País</label>
                            <input type="text" class="form-control-lg form-control-plaintext text-uppercase" id="pais_laboral_suscriptor" name="pais_laboral_suscriptor" readonly value="Colombia" autocomplete="off" />
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="departamento_empleo_suscriptor">Departamento</label>
                            <select class="form-control form-control-lg" id="departamento_empleo_suscriptor" name="departamento_empleo_suscriptor" required autocomplete="off">
                                <option value="">Seleccione una opción</option>
                                @foreach (var item in departamento)
                                {
                                    @if (Model.departamento_empleo_suscriptor == item.IdEstado.ToString())
                                    {
                                        <option selected="selected" value="@item.IdEstado" data-idsiicon="@item.IdSiicon">@item.Descripcion</option>
                                    }
                                    else
                                    {
                                        <option value="@item.IdEstado" data-idsiicon="@item.IdSiicon">@item.Descripcion</option>
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="ciudad_empleo_suscriptor">Ciudad</label>
                            <select class="form-control form-control-lg text-uppercase" id="ciudad_empleo_suscriptor" name="ciudad_empleo_suscriptor" required autocomplete="off">
                                <option value="">Seleccione una opción</option>
                                <option value="@Model.ciudad_empleo_suscriptor" selected>@ViewData["CiudadLaboralSuscriptor"]</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col-12 col-md-6">
                            <label for="telefono_empleo_suscriptor">Teléfono</label>
                            <input type="tel" class="form-control form-control-lg" id="telefono_empleo_suscriptor" name="telefono_empleo_suscriptor" value="@Model.telefono_empleo_suscriptor" autocomplete="off" />
                        </div>
                        <div class="col-12 col-md-6">
                            <label for="celular_empleo_suscriptor">Celular/Móvil</label>
                            <input type="tel" class="form-control form-control-lg" id="celular_empleo_suscriptor" name="celular_empleo_suscriptor" value="@Model.celular_empleo_suscriptor" autocomplete="off" />
                        </div>
                    </div>

                    <div class="form-row mt-4">
                        <div class="col-12 col-md-4">
                            <label for="profesion_suscriptor">Profesión o actividad económica</label>
                            <input type="text" class="form-control form-control-lg text-uppercase" id="profesion_suscriptor" name="profesion_suscriptor" required value="@Model.profesion_suscriptor" autocomplete="off" />
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="envio_correspondencia_suscriptor">Envío de correspondencia</label>
                            <select class="form-control form-control-lg" id="envio_correspondencia_suscriptor" name="envio_correspondencia_suscriptor" required autocomplete="off">
                                <option value="">Seleccione una opción</option>
                                @(Model.envio_correspondencia_suscriptor == "domicilio" ? Html.Raw("<option value=\"domicilio\"  selected=\"selected\">Domicilio</option>") : Html.Raw("<option value=\"domicilio\">Domicilio</option>"))
                                @(Model.envio_correspondencia_suscriptor == "oficina" ? Html.Raw("<option value=\"oficina\"  selected=\"selected\">Oficina</option>") : Html.Raw("<option value=\"vichada\">oficina</option>"))
                                @(Model.envio_correspondencia_suscriptor == "correoelectronico" ? Html.Raw("<option value=\"correoelectronico\"  selected=\"selected\">Correo electrónico</option>") : Html.Raw("<option value=\"correoelectronico\">Correo electrónico</option>"))
                            </select>
                        </div>
                        <div class="col-12 col-md-4">
                            <label for="email_suscriptor">Correo electrónico</label>
                            <input type="email" class="form-control form-control-lg text-uppercase" id="email_suscriptor" name="email_suscriptor" required value="@Model.email_suscriptor" autocomplete="off" />
                        </div>
                    </div>
                </section>
                <!--#endregion-->

                <div class="form-row mt-2 @(Model.documento_identidad_suscriptor_conjunto > 0 ? Html.Raw("d-none") : Html.Raw("")) ">
                    <div class="col">
                        <a class="btn btn-azul text-white" id="isSuscriptorConjunto"><i class="fas fa-plus"></i> Agregar suscriptor conjunto</a>
                    </div>
                </div>

                <section class="@(Model.documento_identidad_suscriptor_conjunto > 0 ? Html.Raw("") : Html.Raw("d-none") )" id="SuscriptorConjuntoSection">

                    <!--#region suscriptor conjunto-->
                    <section>
                        <div class="form-row mt-4 mb-2">
                            <div class="col">
                                <div class="bg-azul p-3 mb-2 text-white text-center">
